<?xml version="1.0" encoding="UTF-8"?>

<Extension>

	<name>coresystem</name>
	<version>0.1</version>
	<priority>3</priority>
	
	<title>核心系统</title>
	<description>用户系统,后台frame,等等</description>
	
	<class>org.opencomb.coresystem.CoreSystem</class>
	<classSetup>org.opencomb.coresystem.CoreSystemSetup</classSetup>
	
	<package folder="/class" namespace="org.opencomb.coresystem" />

	<publicFolder folder="/public" />
	
	<template folder="/template" />
	
	<beanFolder folder="/bean" />
	
	<language locale="zh_CN" path="/path/in/extension/folder/aadd" />
	<language locale="zh_TW" path="/path/in/extension/folder/aadd" />
	
	<!-- 依赖脚本语言-->
	<require type="language" item="php">
	<![CDATA[
		>=5.3,<6.0
	]]>
	</require>
	
	<!-- 依赖脚本语言模块-->

	<!-- 依赖蜂巢系统-->
	<require type="platform"><![CDATA[ >=0.1 ]]></require>
	
	<!-- 依赖其他扩展-->
	
	<!-- 支持其他扩展-->
	<support type="extension" item="com.companyC.extensionA">
		<version value="1.4" />
		<version value="1.5" />
		<version scope="2->3" />
	</support>


</Extension>
